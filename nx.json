{
  "implicitDependencies": {},
  "affected": {
    "defaultBase": "last-release"
  },
  "npmScope": "fdj-ca",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "ssr",
          "e2e",
          "swagger"
        ],
        "cacheDirectory": ".nxcache",
        "accessToken": "MmI4NzZjMmUtZDc2ZS00ZTM3LThhOTAtNjA1ZWRiMTQ3ODIyfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "workspaceLayout": {
    "libsDir": "libs",
    "appsDir": "projects"
  },
  "generators": {
